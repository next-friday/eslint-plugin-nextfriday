{
  "name": "eslint-plugin-nextfriday",
  "version": "1.0.2",
  "description": "A comprehensive ESLint plugin providing custom rules and configurations for Next Friday development workflows.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "nextfriday",
    "next-friday",
    "linting",
    "code-quality",
    "javascript",
    "typescript",
    "development-tools"
  ],
  "homepage": "https://github.com/next-friday/eslint-plugin-nextfriday",
  "bugs": {
    "url": "https://github.com/next-friday/eslint-plugin-nextfriday/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/next-friday/eslint-plugin-nextfriday.git"
  },
  "license": "MIT",
  "author": "Next Friday <nextfriday.developer@gmail.com>",
  "contributors": [
    "@joetakara"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "docs",
    "lib"
  ],
  "scripts": {
    "build": "tsup",
    "changeset": "changeset",
    "changeset:publish": "npm publish --provenance --access public",
    "changeset:version": "changeset version",
    "clear": "rm -rf lib node_modules/.cache .eslintcache",
    "eslint": "eslint src --ext .js,.ts,.mjs --fix",
    "eslint:check": "eslint src --ext .js,.ts,.mjs",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "sort-package-json": "pnpm exec sort-package-json",
    "sort-package-json:check": "pnpm exec sort-package-json --check",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.42.0",
    "emoji-regex": "^10.5.0",
    "tsup": "^8.5.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.6",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.35.0",
    "@jest/globals": "^30.1.2",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@swc/core": "^1.13.5",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.5.4",
    "@types/ramda": "^0.31.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@typescript-eslint/rule-tester": "^8.42.0",
    "eslint": "^9.35.0",
    "eslint-config-airbnb-extended": "^2.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-sonarjs": "^3.0.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "sort-package-json": "^3.4.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.42.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
